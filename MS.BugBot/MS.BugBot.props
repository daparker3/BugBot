<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="12.0" DefaultTargets="Build" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup Condition="'$(Platform)' == 'x64'">
    <DebuggerPath Condition="'$(DebuggerPath)'==''">$(DebuggerRoot)</DebuggerPath>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Platform)' == 'x86'">
    <DebuggerPath Condition="'$(DebuggerPath)'==''">$(DebuggerRoot_x86)</DebuggerPath>
  </PropertyGroup>
  <PropertyGroup>
    <DebuggerPath Condition="'$(DebuggerPath)'==''">C:\Debuggers</DebuggerPath>
  </PropertyGroup>
</Project>